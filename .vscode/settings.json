{
    "git.enableSmartCommit": true,
    "al.enableCodeActions": true,
    "al.enableCodeAnalysis": true,

    "al.codeAnalyzers": [
        "${CodeCop}",
        //"${PerTenantExtensionCop}",
        //"${AppSourceCop}",
        "${UICop}",
    ],

    "al.ruleSetPath": "./.codeAnalysis/al.ruleset.json",
    "CRS.ObjectNamePrefix": "TestMAH-",
    "CRS.FileNamePattern": "<ObjectTypeShort><ObjectId>.<ObjectNameShort>.al",
    "CRS.FileNamePatternExtensions": "<ObjectTypeShort><BaseId>-<ObjectId>-<ObjectNameShort>Ext.al",
    "CRS.OnSaveAlFileAction": "Rename",
}
